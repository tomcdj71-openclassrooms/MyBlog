{% extends "base.html.twig" %}
{% block body %}
	<section class="section register min-vh-100 d-flex flex-column align-items-center justify-content-center py-4">
		<div class="container">
			<div class="row justify-content-center">
				<div class="col-lg-4 col-md-6 d-flex flex-column align-items-center justify-content-center">
					<div class="card mb-3">
						<div class="card-body">
							<div class="pt-4 pb-2">
								<h5 class="card-title text-center pb-0 fs-4">Login to Your Account</h5>
								<p class="text-center small">Enter your username &amp; password to login</p>
							</div>
							<form class="row g-3 needs-validation" novalidate="" data-np-autofill-type="login" data-np-checked="1" data-np-watching="1">
								<div class="col-12">
									<label for="yourUsername" class="form-label">Username</label>
									<div class="input-group has-validation">
										<span class="input-group-text" id="inputGroupPrepend">@</span>
										<input type="text" name="username" class="form-control" id="yourUsername" required="" data-np-autofill-type="username" data-np-uid="4b6d0c35-080d-4612-a8f6-b1ee381523f8" autocomplete="off" data-np-checked="1">
										<div class="invalid-feedback">Please enter your username.</div>
										<span data-np-uid="4b6d0c35-080d-4612-a8f6-b1ee381523f8" style="width: 24px; min-width: 24px; height: 24px; background-image: url(&quot;moz-extension://fd2768e8-0512-443f-a150-1b711559b771/assets/manifestIcons/icon.svg&quot;); background-repeat: no-repeat; background-position: left center; background-size: auto; border: medium none; display: inline-block; visibility: visible; position: absolute; cursor: pointer; z-index: 998; padding: 0px; transition: none 0s ease 0s; pointer-events: all; right: 0px; left: 324px; top: 6.00003px; min-height: 24px;"></span>
									</div>
								</div>
								<div class="col-12">
									<label for="yourPassword" class="form-label">Password</label>
									<input type="password" name="password" class="form-control" id="yourPassword" required="" data-np-autofill-type="password" data-np-uid="4c80495a-be99-43bf-add3-ccf6e603788b" autocomplete="off" data-np-checked="1">
									<div class="invalid-feedback">Please enter your password!</div>
									<span data-np-uid="4c80495a-be99-43bf-add3-ccf6e603788b" style="width: 24px; min-width: 24px; height: 24px; background-image: url(&quot;moz-extension://fd2768e8-0512-443f-a150-1b711559b771/assets/manifestIcons/icon.svg&quot;); background-repeat: no-repeat; background-position: left center; background-size: auto; border: medium none; display: inline-block; visibility: visible; position: absolute; cursor: pointer; z-index: 998; padding: 0px; transition: none 0s ease 0s; pointer-events: all; right: 0px; left: 343px; top: 249.8px; min-height: 24px;"></span>
								</div>
								<div class="col-12">
									<div class="form-check">
										<input class="form-check-input" type="checkbox" name="remember" value="true" id="rememberMe" data-np-checked="1">
										<label class="form-check-label" for="rememberMe">Remember me</label>
									</div>
								</div>
								<div class="col-12">
									<button class="btn btn-primary w-100" type="submit">Login</button>
								</div>
								<div class="col-12">
									<p class="small mb-0">Don't have account?
										<a href="pages-register.html">Create an account</a>
									</p>
								</div>
							</form>
						</div>
					</div>
				</div>
			</div>
		</div>
	</section>
{% endblock %}
